{
  "package": {
    "name": "${project.name}",
    "repo": "research-virtualfortknox",
    "subject": "research-virtualfortknox",
    "website_url": "${project.url}",
    "issue_tracker_url": "https://github.com/research-virtualfortknox/msb-client-websocket-java/issues",
    "vcs_url": "https://github.com/research-virtualfortknox/msb-client-websocket-java.git",
    "github_use_tag_release_notes": true,
    "licenses": [
      "Apache-2.0"
    ],
    "public_download_numbers": false,
    "public_stats": false
  },
  "version": {
    "name": "${project.version}",
    "released": "${maven.build.timestamp}",
    "vcs_tag": "${project.version}"
  },
  "files": [
    {
      "includePattern": "pom.xml",
      "uploadPattern": "com/github/research-virtualfortknox/msb-client-parent/${project.version}/msb-client-parent-${project.version}.pom"
    },
    {
      "includePattern": "msb-client-api/target/(.*.jar)",
      "uploadPattern": "com/github/research-virtualfortknox/msb-client-api/${project.version}/$1"
    },
    {
      "includePattern": "msb-client-api/pom.xml",
      "uploadPattern": "com/github/research-virtualfortknox/msb-client-api/${project.version}/msb-client-api-${project.version}.pom"
    },
    {
      "includePattern": "msb-client-websocket/target/(.*.jar)",
      "uploadPattern": "com/github/research-virtualfortknox/msb-client-websocket/${project.version}/$1"
    },
    {
      "includePattern": "msb-client-websocket/pom.xml",
      "uploadPattern": "com/github/research-virtualfortknox/msb-client-websocket/${project.version}/msb-client-websocket-${project.version}.pom"
    }
  ],
  "publish": true
}